<h1>Let's edit a contact</h1>

<form action="/contacts/<%= @contact['id'] %>" method="post">
  <label for="first">First Name</label><br>
  <input type="text" name="first" value="<%= @contact['first'] %>"><br>
  <label for="last">Last Name</label><br>
  <input type="text" name="last" value="<%= @contact['last'] %>"><br>
  <label for="age">Age</label><br>
  <input type="number" name="age" value="<%= @contact['age'] %>"><br>
  <button>Edit Contact</button>
</form>